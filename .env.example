# MultiversX Network Configuration
NEXT_PUBLIC_MULTIVERSX_CHAIN=devnet
NEXT_PUBLIC_GATEWAY_URL=https://devnet-gateway.multiversx.com
NEXT_PUBLIC_API_URL=https://devnet-api.multiversx.com
NEXT_PUBLIC_EXPLORER_URL=https://devnet-explorer.multiversx.com

# Wallet Configuration
NEXT_PUBLIC_WALLET_CONNECT_BRIDGE_URL=https://bridge.walletconnect.org
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_project_id_here

# IPFS Configuration
NEXT_PUBLIC_IPFS_GATEWAY=https://gateway.pinata.cloud
PINATA_API_KEY=your_pinata_key_here
PINATA_SECRET_KEY=your_pinata_secret_here

# AI Services (Free Tier)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
GOOGLE_AI_API_KEY=your_google_ai_key_here

# Analytics & Monitoring (Free Tier)
NEW_RELIC_LICENSE_KEY=your_new_relic_key_here
SENTRY_DSN=your_sentry_dsn_here
LOGROCKET_APP_ID=your_logrocket_id_here

# Development
NODE_ENV=development
NEXT_PUBLIC_APP_NAME=PROTOGX NETWORK
NEXT_PUBLIC_APP_VERSION=1.0.0

# Game Configuration
NEXT_PUBLIC_ENABLE_AI_AGENTS=true
NEXT_PUBLIC_ENABLE_NFT_REWARDS=true
NEXT_PUBLIC_ENABLE_LEADERBOARDS=true
NEXT_PUBLIC_MAX_PLAYERS_PER_GAME=100

# Security
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000